{
  "name": "@nivite/sdk",
  "version": "1.0.5",
  "description": "Create your own invite - the nerdy way",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "private": false,
  "scripts": {
    "build": "tsc",
    "test": "jest --config jestconfig.json",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "files": [
    "lib/**/*"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nivite/sdk.git"
  },
  "keywords": [
    "invite",
    "rsvp",
    "wedding",
    "birthday",
    "graduation",
    "festival",
    "diy",
    "party",
    "gettogether",
    "potluck",
    "celebration",
    "picnic"
  ],
  "author": "Anand Rockzz",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nivite/sdk/issues"
  },
  "homepage": "https://github.com/nivite/sdk#readme",
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@types/jest": "^24.0.15",
    "jest": "^24.8.0",
    "prettier": "^1.18.2",
    "ts-jest": "^24.0.2",
    "tslint": "^5.17.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.5.2"
  },
  "dependencies": {
    "@types/file-saver": "^2.0.1",
    "file-saver": "^2.0.2",
    "moment": "^2.24.0",
    "query-string": "^6.7.0"
  }
}
